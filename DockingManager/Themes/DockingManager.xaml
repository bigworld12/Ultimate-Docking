<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:DockingManager">
   

   <ControlTemplate x:Key="DockPanelSplitterTemplate" TargetType="{x:Type local:DockingManager}">
      <ContentPresenter Content="{TemplateBinding Content}"></ContentPresenter>
   </ControlTemplate>


   <Style TargetType="{x:Type local:DockingManager}">
      <Setter Property="Template" Value="{StaticResource DockPanelSplitterTemplate}"/>
   </Style>
</ResourceDictionary>
